//http://processing.org/learning/basics/piechart.html

size(200, 200);
background(100);
smooth();
noStroke();

var diameter = 150;
var angs = [30, 10, 45, 35, 60, 38, 75, 67];
var lastAng = 0;

for (var i=0; i<angs.length; i++){
  fill(angs[i] * 3.0);
  arc(width/2, height/2, diameter, diameter, lastAng, lastAng+radians(angs[i]));
  lastAng += radians(angs[i]);  
}